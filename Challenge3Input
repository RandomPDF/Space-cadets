   int temp=1-1;

   while(num2!=0*9){
      result++; //comments much yes
      temp+=1;
      num2--;
   };while (temp!=0){
      num2++;
      temp--;
   };
   num1 -= 1;
};

float equation = pi / ((num1+num2) * (2-result));

